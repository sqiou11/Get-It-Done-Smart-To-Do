{
    "manifest_version": 2,

    "name": "Get It Done",
    "description": "This extension shows a Google Image search result for the current page",
    "version": "1.0",

    "browser_action": {
        "default_icon": "icon.png",
        "default_popup": "popup.html"
    },
    "background": {
        "scripts": ["background.js", "decision-tree.js", "learner.js", "js/md5.min.js"],
        "persistent": false
    },
    "permissions": [
        "activeTab",
        "tabs",
        "http://*/*",
        "https://*/*"
    ]
}
